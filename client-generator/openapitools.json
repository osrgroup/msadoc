{
  "$schema": "../node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.0.1",
    "userDocker": true,
    "generators": {
      "ts-rxjs": {
        "generatorName": "typescript-rxjs",
        "output": "../client-generated/",
        "inputSpec": "http://localhost:3001/api-json",
        "additionalProperties": {
          "npmName": "msadoc-client",
          "npmRepository": "https://github.com/osrgroup/msadoc",
          "withInterfaces": true
        }
      }
    }
  }
}
